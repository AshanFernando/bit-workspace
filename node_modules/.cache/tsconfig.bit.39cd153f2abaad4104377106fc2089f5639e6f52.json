{
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom",
      "dom.Iterable"
    ],
    "target": "es2020",
    "module": "NodeNext",
    "jsx": "react-jsx",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./dist"
  },
  "exclude": [
    "../../commmunity/examples/acme-web/artifacts",
    "../../commmunity/examples/acme-web/public",
    "../../commmunity/examples/acme-web/dist",
    "../../commmunity/examples/acme-web/node_modules",
    "../../commmunity/examples/acme-web/package.json",
    "../../commmunity/examples/acme-web/**/*.cjs",
    "../../commmunity/examples/acme-web/./dist"
  ],
  "include": [
    "../../commmunity/examples/acme-web/**/*",
    "../../commmunity/examples/acme-web/**/*.json",
    "./global-types/**/*"
  ]
}