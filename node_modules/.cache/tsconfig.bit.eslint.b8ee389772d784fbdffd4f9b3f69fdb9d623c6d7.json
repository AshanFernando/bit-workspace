{
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom",
      "dom.Iterable"
    ],
    "target": "es2020",
    "module": "NodeNext",
    "jsx": "react-jsx",
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "moduleResolution": "NodeNext",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "outDir": "./dist"
  },
  "exclude": [
    "../../commmunity/examples/platform/artifacts",
    "../../commmunity/examples/discussion-server/artifacts",
    "../../commmunity/examples/gateway-server/artifacts",
    "../../commmunity/examples/user-server/artifacts",
    "../../commmunity/examples/platform/public",
    "../../commmunity/examples/discussion-server/public",
    "../../commmunity/examples/gateway-server/public",
    "../../commmunity/examples/user-server/public",
    "../../commmunity/examples/platform/dist",
    "../../commmunity/examples/discussion-server/dist",
    "../../commmunity/examples/gateway-server/dist",
    "../../commmunity/examples/user-server/dist",
    "../../commmunity/examples/platform/node_modules",
    "../../commmunity/examples/discussion-server/node_modules",
    "../../commmunity/examples/gateway-server/node_modules",
    "../../commmunity/examples/user-server/node_modules",
    "../../commmunity/examples/platform/package.json",
    "../../commmunity/examples/discussion-server/package.json",
    "../../commmunity/examples/gateway-server/package.json",
    "../../commmunity/examples/user-server/package.json",
    "../../commmunity/examples/platform/**/*.cjs",
    "../../commmunity/examples/discussion-server/**/*.cjs",
    "../../commmunity/examples/gateway-server/**/*.cjs",
    "../../commmunity/examples/user-server/**/*.cjs",
    "../../commmunity/examples/platform/./dist",
    "../../commmunity/examples/discussion-server/./dist",
    "../../commmunity/examples/gateway-server/./dist",
    "../../commmunity/examples/user-server/./dist"
  ],
  "include": [
    "../../commmunity/examples/platform/**/*",
    "../../commmunity/examples/discussion-server/**/*",
    "../../commmunity/examples/gateway-server/**/*",
    "../../commmunity/examples/user-server/**/*",
    "../../commmunity/examples/platform/**/*.json",
    "../../commmunity/examples/discussion-server/**/*.json",
    "../../commmunity/examples/gateway-server/**/*.json",
    "../../commmunity/examples/user-server/**/*.json",
    "../../commmunity/examples/platform/.mocharc.js",
    "../../commmunity/examples/discussion-server/.mocharc.js",
    "../../commmunity/examples/gateway-server/.mocharc.js",
    "../../commmunity/examples/user-server/.mocharc.js",
    "./global-types/**/*"
  ]
}